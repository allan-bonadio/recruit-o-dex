{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"indent": ["warn", "tab"],
		"linebreak-style": ["error", "unix"],
		"quotes": ["off", "single"],
		"semi": ["warn", "never"]
		"eqeqeq": ["off", "never"]
	}
}